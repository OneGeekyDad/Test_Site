<!DOCTYPE html>
<html>
<head>
  <title>Blog Home</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <nav>
    <a href="/">Home</a>
    <a href="/new">New Post</a>
  </nav>
  <div class="container">
    <h1>All Posts</h1>
    <% posts.forEach(post => { %>
      <div class="post">
        <h2><a href="/post/<%= post._id %>"><%= post.title %></a></h2>
      </div>
    <% }) %>
  </div>
</body>
</html>
